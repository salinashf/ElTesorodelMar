<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="micromobil.eltesorodelmar">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.helper.CastleActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.helper.Castle"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".activity.helper.Credits"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".activity.helper.LoadResources"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".activity.helper.Story"
            android:screenOrientation="landscape"></activity>

    </application>
</manifest>